<% if logged_in? %>
  <%= @user.nickname %>
  <%= link_to 'mypage', user_show_path(@user.username) %>
  <%= link_to 'logout', logout_path, method: :delete %>
<% else %>
  <%= link_to 'login', login_path, method: :get %>
<% end %>
<% @latest_posts.each do |post| %>
  <%= link_to post.title, post_show_path(post.user.username, post.id), method: :get %>
<% end %>

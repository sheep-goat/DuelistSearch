<% unless logged_in? %>
  <%= render partial: 'non_login' %>
<% end %>
<div id="latest-posts-list">
  <h3 class="landing-page">最新の投稿</h3>
  <div class="latest-posts landing-block">
    <% @latest_posts.each do |post| %>
      <%= render partial: 'layouts/post_panel', locals: { post: post } %>
    <% end %>
  </div>
</div>
